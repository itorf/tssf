<table class="table table-bordered table-striped header-fixed" id="show-dates">
	<thead>
		<tr>
			<th colspan="4">Upcoming Shows</th>
		</tr>
		<tr>
			<th>Date</th>
			<th>Venue</th>
			<th>Location</th>
			<th>Tickets</th>
		</tr>
	</thead>
	<tbody>
		<% shows.forEach(function (show) { %>
			<tr>
				<td><%= show.escape('date') %></td>
				<td><%= show.escape('venue') %></td>
				<td><%= show.escape('city') %>, <%= show.escape('state') %></td>
				<td>
					<form action=""></form>
					<button type="button" class="btn btn-xs tix" data-url='<%= show.escape('tickets_url') %>'>
						Buy Tickets
					</button>
				</td>
			</tr>
		<% }) %>
		<tr><td colspan="4"><button class="btn btn-sm more-shows">Show More</button></td></tr>
	</tbody>
</table>